{
    "manifest_version" : 2,
    "name" : "Read Webpage Content" ,
    "version" : "1.0",
    "description" : "Read the contents of the webppage",
    "Permissions":[
        "activeTab"
    ],
    "browser_action" : {
        "default_popup" : "popup.html"
    },
    "icons" : {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_scripts":[
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
    

    
}