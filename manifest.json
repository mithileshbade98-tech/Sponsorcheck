{
    "manifest_version": 2,
    "name": "Read Webpage Content",
    "version": "1.0",
    "description": "Reads the content of the web page",
    "permissions": [
      "activeTab"
    ],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
  